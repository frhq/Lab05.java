import java.util.*;
public class Act3 {
    public static void main(String [] args) {
     Scanner input = new Scanner(System.in);

     int n;
     int num = 1;
     System.out.print("Enter the number of integers you want to enter: ");
     n = input.nextInt();
  
      int count = 0;
      System.out.print("Enter " + n + " integers.");
        for (int i = 1; i <= n ; i++){
         num = input.nextInt();
           if (num==0) {
            count++;}
          }
            System.out.print("The number of zereos are: " + count);
    }
}
